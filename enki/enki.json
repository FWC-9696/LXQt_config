{
    "CodeChat": {
        "Enabled": false
    },
    "Lint": {
        "Python": {
            "Enabled": true,
            "IgnoredMessages": "",
            "MaxLineLength": 79,
            "Path": "flake8",
            "Show": "all"
        }
    },
    "Modes": {
        "SML": {
            "Enabled": "whenOpened",
            "InterpreterPath": "sml"
        },
        "Scheme": {
            "Enabled": "whenOpened",
            "InterpreterPath": "scheme"
        }
    },
    "Navigator": {
        "CtagsPath": "ctags",
        "Enabled": true,
        "SortAlphabetically": false
    },
    "NegativeFileFilter": [
        ".*",
        "*~",
        "*.o",
        "*.pyc",
        "*.bak",
        "__pycache__",
        "*.class"
    ],
    "OpenTerm": {
        "Term": ""
    },
    "PlatformDefaultsHaveBeenSet": true,
    "Preview": {
        "Enabled": true,
        "JavaScriptEnabled": true,
        "Template": "Default"
    },
    "Qutepart": {
        "AutoCompletion": {
            "Enabled": true,
            "Threshold": 3
        },
        "EOL": {
            "AutoDetect": true,
            "Mode": "\\n"
        },
        "Edge": {
            "Color": "#ff0000",
            "Column": 80,
            "Enabled": false
        },
        "Font": {
            "Family": "Monospace",
            "Size": 12
        },
        "Indentation": {
            "AutoDetect": true,
            "UseTabs": false,
            "Width": 4
        },
        "StripTrailingWhitespace": false,
        "VimModeEnabled": false,
        "WhiteSpaceVisibility": {
            "Any": false,
            "Incorrect": true
        },
        "Wrap": {
            "Enabled": true,
            "Mode": "WrapAtWord"
        }
    },
    "Sphinx": {
        "AdvancedMode": false,
        "BuildOnSave": false,
        "Cmdline": "sphinx-build -d _build/doctrees . _build/html",
        "Enabled": false,
        "Executable": "sphinx-build",
        "OutputPath": "_build/html",
        "ProjectPath": "",
        "SourcePath": "."
    },
    "_version": 21
}